<h3> TinyApp </h3>

  <% if (!username) {%>
<form action="/login" method="POST">
    <input id="username" type="" name="username"
          placeholder="" action="" style="width: 300px">
    <input type="submit" value="Sign In" />
</form>
  <% } else { %>
<form action="/logout" method="POST">
    <span> <%= username %> </span>
    <input id="username" type="submit" value="Sign Out" />
</form>

  <% } %>